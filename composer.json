{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/asset": "^7.3.0",
        "symfony/asset-mapper": "^7.3.5",
        "symfony/console": "^7.3.6",
        "symfony/dotenv": "^7.3.2",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "^7.3.6",
        "symfony/runtime": "^7.3.4",
        "symfony/twig-bundle": "^7.3.4",
        "symfony/yaml": "^7.3.5",
        "twig/extra-bundle": "^2.12|^3.22.1",
        "twig/twig": "^2.12|^3.22"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "friendsofphp/php-cs-fixer": "^3.89.2",
        "phpunit/phpunit": "^12.4.3",
        "rector/rector": "^2.2.8",
        "symfony/browser-kit": "^7.3.6",
        "symfony/css-selector": "^7.3.6",
        "symfony/debug-bundle": "^7.3.5",
        "symfony/maker-bundle": "^1.64",
        "symfony/monolog-bundle": "^3.10",
        "symfony/stopwatch": "^7.3.0",
        "symfony/web-profiler-bundle": "^7.3.5",
        "zenstruck/foundry": "^2.8"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "fixer": "./vendor/bin/php-cs-fixer",
        "rector:check": "./vendor/bin/rector process --dry-run",
        "rector:fix": "./vendor/bin/rector process",
        "tidy": [
            "@rector:fix",
            "@fixer fix"
        ],
        "test": "./bin/phpunit --testdox"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.0.0",
            "docker": true
        }
    }
}
